{
  "name": "作業しろ"
, "version": "1.0.0"
, "manifest_version": 2
, "description": "作業しましょうね"
, "icons": {
     "16": "icons/icon16.png"
  ,  "48": "icons/icon48.png"
  , "128": "icons/icon128.png"
  }
, "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ]
    , "js": [
        "js/jquery-2.1.3.min.js"
      , "js/injector.js"
      ]
    }
  ]
, "permissions": [
    "<all_urls>"
  , "storage"
  ]
, "browser_action": {
    "default_icon": "icons/icon16.png"
  , "default_title": "作業しろ"
  , "default_popup": "html/popup.html"
  }
, "background": {
    "scripts": ["js/background.js"]
  }
}
