{
  "name": "作業しろ"
, "version": "0.0.1"
, "manifest_version": 2
, "description": "作業しましょうね"
, "icons": {
     "16": "icons/icon16.png"
  ,  "48": "icons/icon48.png"
  , "128": "icons/icon128.png"
  }
, "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ]
    , "js": [
        "jquery-2.1.3.min.js"
      , "injector.js"
      ]
    }
  ]
, "web_accessible_resources": [
    "executor.js"
  , "blacklist.json"
  ]
, "permissions": [
    "<all_urls>"
  ]
}
